{
  "name": "portfolio",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@babel/cli": "^7.0.0-beta.53",
    "@babel/core": "^7.0.0-beta.53",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.53",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.53",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.53",
    "@babel/preset-env": "^7.0.0-beta.53",
    "@babel/preset-react": "^7.0.0-beta.53",
    "@babel/preset-stage-3": "^7.0.0-beta.53",
    "@babel/register": "^7.0.0-beta.53",
    "@babel/runtime": "^7.0.0-beta.53",
    "autoprefixer": "^8.0.0",
    "axios": "^0.18.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^8.0.0-beta.4",
    "babel-preset-react-app": "^4.0.0-next.b2fd8db8",
    "case-sensitive-paths-webpack-plugin": "2.1.1",
    "chalk": "^2.3.1",
    "classnames": "^2.2.5",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.10",
    "dotenv": "^5.0.0",
    "es6-promise": "^4.2.4",
    "file-loader": "^1.1.6",
    "fs-extra": "3.0.1",
    "node-sass": "^4.8.3",
    "object-assign": "^4.1.1",
    "path-parser": "^4.0.4",
    "postcss-flexbugs-fixes": "^3.3.0",
    "postcss-loader": "^2.1.3",
    "promise": "^8.0.1",
    "prop-types": "^15.6.0",
    "query-string": "^5.1.0",
    "raf": "^3.4.0",
    "react": "^16.3.1",
    "react-cookie-banner": "^3.0.0",
    "react-dom": "^16.3.1",
    "react-ga": "^2.4.1",
    "react-helmet": "^5.2.0",
    "react-loading-components": "^1.1.12",
    "react-stripe-elements": "^1.6.0",
    "regenerator-runtime": "^0.11.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.20.2",
    "url-loader": "^1.0.1",
    "whatwg-fetch": "^2.0.4"
  },
  "scripts": {
    "start": "node scripts/start.js",
    "build": "node scripts/build.js",
    "build-ci": "CI=true node scripts/build.js",
    "clean-ci": "rm -rf build coverage",
    "test": "node scripts/test.js --env=jsdom",
    "coverage": "node scripts/test.js --env=jsdom --coverage",
    "test-ci": "CI=true node scripts/test.js --env=jsdom --coverage",
    "ci": "yarn clean-ci && yarn test-ci && yarn build-ci"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "setupFiles": [
      "<rootDir>/config/polyfills.js"
    ],
    "testMatch": [
      "<rootDir>/test/**/__tests__/**/*.{js,jsx,mjs}",
      "<rootDir>/test/**/?(*.)(spec|test).{js,jsx,mjs}"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(js|jsx|mjs)$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.scss$": "<rootDir>/config/jest/cssTransform.js",
      "^(?!.*\\.(js|jsx|mjs|css|json|scss)$)": "<rootDir>/config/jest/fileTransform.js"
    },
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx|mjs)$"
    ],
    "moduleNameMapper": {
      "^.+\\.scss$": "identity-obj-proxy",
      "^react-native$": "react-native-web"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/test/setupTests.js",
    "moduleFileExtensions": [
      "web.js",
      "mjs",
      "js",
      "json",
      "web.jsx",
      "jsx",
      "node"
    ],
    "unmockedModulePathPatterns": [
      "react",
      "enzyme"
    ]
  },
  "babel": {
    "presets": [
      "react-app",
      [
        "@babel/env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "safari >= 7"
            ]
          }
        }
      ],
      "@babel/stage-3"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-proposal-object-rest-spread",
      "@babel/plugin-syntax-optional-chaining"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "devDependencies": {
    "@babel/plugin-syntax-optional-chaining": "^7.0.0-beta.53",
    "babel-jest": "^22.4.3",
    "babel-plugin-flow-react-proptypes": "^22.0.0",
    "codecov": "^3.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-react-app": "^3.0.0-next.b2fd8db8",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-babel": "^5.0.0",
    "eslint-plugin-flowtype": "^2.46.1",
    "eslint-plugin-import": "^2.10.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.7.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "html-webpack-plugin": "3.2.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.3",
    "randomstring": "^1.1.5",
    "react-dev-utils": "^5.0.1",
    "react-test-context-provider": "^2.2.0",
    "react-test-renderer": "^16.3.1",
    "sw-precache-webpack-plugin": "0.11.5",
    "webpack": "^3.11.0",
    "webpack-dev-server": "^2.11.2",
    "webpack-manifest-plugin": "1.3.2"
  }
}
